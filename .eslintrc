{
  "extends": [
    "react-app",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "import/prefer-default-export": "off",
    "react-hooks/exhaustive-deps": "warn",
    "no-underscore-dangle": "off",
    "@typescript-eslint/comma-dangle": "off",
    "@typescript-eslint/indent": "off",
    "no-param-reassign": "off"
  },
  "ignorePatterns": [
    "lighthouse/**",
    "build/**"
  ]
}